<h1>All Kittens</h1>

<ul>
  <% @kittens.each do |kitten| %>
    <li>
      <%= link_to kitten.name, kitten_path(kitten)%> || <%= link_to "Edit Kitten", edit_kitten_path(kitten) %> || <%= link_to "Delete Kitten", kitten_path(kitten), method: :delete, data: {confirm: "Really delete this kitten?"} %>
    </li>
  <% end %>
</ul>